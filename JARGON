(?:s+h+)?r+e+k+t+ # 'shrekt', 'rekt'
smf?h
snafu
s+o+
spo+py # 'spoopy', 'spoooopy', etc
s+t+f+u+
tb[hf]
tb
th?(?:ankz|n?x) # 'tnx', 'tx', 'thankz', 'tankz', 'thnx', ...
t+h+i+c+ # T H I C C
thott?(?:ie|y)? # 'thot', 'thottie', 'thotty'
tfw
tgif
t+i+(?:g+h+t+|t+e+) # 'tight', 'tite'
til
tldr
tmi
tryna
tty?[ls]
ty(?:vm)?
u+[hm]+(?:o+h+)? # 'uuuuuh', 'uuuuuhhhhh', 'uhoh', 'ummmm', etc
vip
vlog
wa*i+f+(?:e*y+|u+) # 'waifuuuu', 'wifey', etc
w+e+l+ # 'welllll', etc
w+h*[au]+t+ # 'whaaaat', 'waaaaat', 'wuuuuuuuutttt', etc
w+h*e+l+p+
w+h*o+a+(?:h+[aeou]+)?h* # 'woahhhh', 'whoahohhoho', etc
w*h*y+ # 'whyyyy', 'wwwhhhyyyyy', 'yyyyyyy', etc
w*o{2,}f+ # 'oof', 'woof', 'wwoooof', etc
w+h*o+s+h+ # 'woosh', 'wooooosh', 'whoosh', etc
w*o+p+s* # 'woops', 'wooooops', 'oooops', 'oop', etc
w+o+w+(?:[sz]+[ae]+[hr]*)?[sz]? # 'wwwoooowww', 'wowserz', 'wowwza', etc
wtc # where to cop
w+t+[fh]+(?:o+m+f*g+)? # 'wtf', 'wwwtf', wtttttf', 'wtfomfg', etc
(?:x+o+)+x*
y+[ae]+e*[hsy]+ # 'yaaaas', 'yeeeeesssss', 'yeahhhh', etc
y+[aeu]+[hp]+ # 'yuuuhh', 'yeeeppp', yaaaahhh', etc
y*[iu]+c+k+(?:i+e+|y+)? # 'ick', 'yucky', etc
y+i+p+(?:i+e+|y+) # 'yippieeee', etc
ymmv
y+o+l+o+
y+tho
yw
z*o+m+f*g+(?:w+t+f+)? # 'omg', 'ooomg', 'omfffg', 'omgwtf', etc
z+o+m+ # 'zooooom', etc

