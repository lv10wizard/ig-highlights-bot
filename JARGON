# 'suuuper', 'superboy', 'superman', 'supergirl', 'superwoman'
s+u+p+e*r+(?:b+[0o]+[iy]+|g+i*r+l+|(?:w+o+)?m+[aey]+n+)?
tb[hf]
tb
th?(?:ankz|n?x) # 'tnx', 'tx', 'thankz', 'tankz', 'thnx', ...
t+h+i+c+ # T H I C C
thott?(?:ie|y)? # 'thot', 'thottie', 'thotty'
