# This file contains one jargon regex per line.
# Anything after '#' is a comment (ignored by code)
# Empty lines and leading/trailing whitespace are also ignored.


\d+[A-Za-z]+(?:\d+me)? # '2spooky4me', '3cute5me', etc
\w+e+l+f+(?:i+e+|e*y+) # 'selfie', 'pelfie', 'selfy', etc
(?:\d+me_?irl\d+)?me_?i+rl # 'me_irl', '2me_irl4meirl', etc
afai(?:cr|cs|ct|k|r) # as far as i ...
afk
a[fs]af*p # 'afap', 'asap', 'asafp'
a+h+w*
(?:a+p+l+e*|b+u+b+l+e*)?[bw]+h*o+t+(?:i+e+|[ae]*y+)s* # 'applebooty', ...
(?:a+p+l+e*|b+u+b+l+e*)?b+u+t+(?:o+c*k+)?s* # 'bubblebutt', ...
aslp?
atm
a+u+t+o+m+a+t+a+ # 'automata' (pychant doesn't consider this as english)
a+w+h*
awol
b+a+e+
badass
(?:b+a+)?z+i+n+g+a*
b+e+c+a+u+s+e+
b+e+(?:a+u+|y+[ou]+)t+[aei]+f+u+l+ # 'beautifullll', 'beeeeyoouteeaafulllll'
b+[ei]+a*y*[ao]+t*c+h+ # 'beeyyyooootch', etc
b+i*e+r+g+a+r+[dt]+e+n+ # 'biergarten'
bitcoin
b+o+ # 'boooooo', ...
br[bls]
b+r+(?:[eau]+|e+w+|o{2,})h* # 'bruh', 'brooooo', 'breeeeehh', etc
bs
bsdm
bsod
btw
b+u+b+l+e* # 'bubbleeee', etc
b+u+l+(?:s+h+e*i+e*t+)? # 'bull', 'buuuullshiiitt', etc
bu+t+e+r+f+a+c+e+
cgi
c+o{2,}l+ # 'cooooool', etc
co+lsto+ry+b+r+(?:[eaou]+|e+w+)h* # coolstorybro
c+u+t+i*e+(?:p+i+e+)?
c+u+[sz]+
d+a+f+u*(?:c*k*|q+) # 'dafuq', 'dafuuuuck', etc
d+a+t+\w{3,} # 'datass', 'daaaateverything', etc
d+[ao]+w*g+
(?:d+a+|t+h+e+)?b+o+m+b*[sz]* # 'thebombbbb', 'bombbsssss'
d+a+(?:y+[au]*h*)?m+n*
d+i+c+k+s*
di+nga+li+ng
d+[ir]+[dn]+l+ # 'dirndl' & misspellings
dmhs
d+o+w+n+(?:b+l+o+[uw]+s+e*|s+h+i+r+t+)s*
d+u+d+e+
# 'asssss', 'asshoooleeeeee', 'dumbasssss', 'dumbassholes', etc
(?:d+u+m+b*)?a+s+(?:h*o+l+)?e*s*
d+u+h+
eli5
emoji
e+n+h+a+n+c+e+
(?:f+a+p+s*)+ # 'fap', 'fapfapfap', 'faaappp', etc
f+a+k+e+
[fg]+a+g+(?:o+t+)? # 'fagggg', 'gagggg', 'fagggootttt', etc
f+a+m+
